{
  "name": "TypescriptTest",
  "version": "0.0.0-semantically-released",
  "description": "",
  "main": "Car.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/typings install",
    "commit": "git-cz",
    "lint": "tslint ./src/{,**/}*.ts",
    "test": "./node_modules/.bin/mocha --opts ./mocha.opts",
    "test:watch": "./node_modules/.bin/mocha -w --opts ./mocha.opts",
    "test:commit": "npm run lint --silent",
    "test:push": "npm run test --silent && npm run lint --silent",
    "build": "./node_modules/.bin/webpack",
    "precommit": "npm run test:commit --silent",
    "prepush": "npm run test:push --silent",
    "security": "./node_modules/.bin/nsp check",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "reflect-metadata": "^0.1.8"
  },
  "devDependencies": {
    "@krux/condition-jenkins": "^1.0.1",
    "chai": "^3.5.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "nsp": "^2.6.2",
    "semantic-release": "^6.3.2",
    "ts-loader": "^1.2.2",
    "ts-node": "^1.7.0",
    "tslint": "^4.0.2",
    "tslint-eslint-rules": "^3.0.0",
    "typescript": "^2.0.10",
    "typings": "^2.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.14.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crywolf/typescript-test.git"
  },
  "publishConfig": {
    "registry": "http://jenkins.billboard.intra:4873/"
  },
  "release": {
    "branch": "master",
    "verifyConditions": {
      "path": "./node_modules/@krux/condition-jenkins"
    }
  }
}
